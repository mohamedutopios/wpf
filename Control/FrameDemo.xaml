<Window x:Class="Control.FrameDemo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Control"
        mc:Ignorable="d"
        Title="FrameDemo" Height="450" Width="800">
    <Grid Background="LightBlue">
        <StackPanel HorizontalAlignment="Left" Width="140">
            <Button Name="btnLebonCoin" Content="LeBonCoin" Width="100" 
                    Height="40" Margin="10" Click="btn_le_bon_coin"/>
            <Button Name="btnAmazon" Content="Amazon" Width="100" 
        Height="40" Margin="10" Click="btn_amazon"/>
            
        </StackPanel>
        <Frame x:Name="frame" Margin="125,0,5,5" Visibility="Hidden" Source="https://www.cdiscount.com"/>
        <Frame x:Name="frame1" Margin="125,0,5,5" Visibility="Hidden" Source="https://www.amazon.fr"/>
    </Grid>
</Window>
